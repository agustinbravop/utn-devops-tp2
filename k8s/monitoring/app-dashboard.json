{
  "id": null,
  "uid": "app-dashboard",
  "title": "Todo App Observability",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": { "from": "now-6h", "to": "now" },
  "tags": ["todo", "devops"],
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "HTTP Requests per Route",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 9 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{namespace=\"app\",service=\"backend\"}[5m])) by (route)",
          "legendFormat": "{{route}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Request Duration P95",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 9 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{namespace=\"app\",service=\"backend\"}[5m])) by (le, route))",
          "legendFormat": "p95 {{route}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      }
    }
  ]
}
